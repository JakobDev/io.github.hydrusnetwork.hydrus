            "name": "python3-PySide6",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "config-opts": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DBUILD_TESTS=OFF"
            ],
            "build-options":{
                "append-path": "/usr/lib/sdk/llvm14/bin",
                "prepend-ld-library-path": "/usr/lib/sdk/llvm14/lib",
                "ldflags":"-pthread -lpthread"
            },
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.qt.io/official_releases/QtForPython/pyside6/PySide6-6.5.1.1-src/pyside-setup-everywhere-src-6.5.1.1.zip",
                    "sha256": "14497661fedfad4ae6b7718744199c8b3b3422566ac75a2dd50028f610b89f13"
                },
                {
                    "type": "shell",
                    "commands": [
                        "mkdir -p /app/include/qt6tmp && cp -R /usr/include/Qt* /app/include/qt6tmp # https://bugreports.qt.io/broswse/PYSIDE-787",
                        "cp /usr/include/pthread.h /app/include/qt6tmp/pthread.h",
                        "sed -i 's|--include-paths=|--include-paths=/app/include/qt6tmp:|' sources/pyside6/cmake/Macros/PySideModules.cmake",

                        "sed -i $'s/\r$//' sources/pyside6/cmake/Macros/PySideModules.cmake # replace CRLF to LF to apply patch for https://bugreports.qt.io/broswse/PYSIDE-2002"
                    ]
                },
                {
                    "type": "patch",
                    "path": "patches/PYSIDE-2002.patch"
                }
            ]
        },
